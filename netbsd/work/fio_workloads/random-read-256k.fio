; random read of 128mb of data

[random-read]
log_avg_msec=1000
readwrite=randwrite
size=4g
write_bw_log=logs/random-read
write_lat_log=logs/random-read
write_iops_log=logs/random-read
thread
ioengine=sync
randrepeat=1
direct=1
; gtod_reduce=1
name=test
filename=test
bs=256k
iodepth=1
; readwrite=randrw
; rwmixread=75
directory=/mnt/test
